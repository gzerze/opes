# vim:ft=plumed
UNITS NATURAL

p: POSITION ATOM=1

METAD ...
  LABEL=metad
  ARG=p.x
  FILE=Hills.data
  PACE=500
  HEIGHT=1
  SIGMA=0.172198
#  BIASFACTOR=10
  GRID_MIN=-3
  GRID_MAX=3
  GRID_BIN=300
  CALC_MAX_BIAS
  CALC_RCT
